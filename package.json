{
  "name": "total-reclaw",
  "version": "1.0.0",
  "description": "long-term memory plugin for openclaw. sqlite + any embedding provider. smart capture, recency-weighted recall, consolidation.",
  "type": "module",
  "main": "dist/index.js",
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.48"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26",
    "sqlite-vec": "*"
  },
  "peerDependenciesMeta": {
    "sqlite-vec": {
      "optional": true
    }
  },
  "keywords": [
    "openclaw",
    "plugin",
    "memory",
    "sqlite",
    "vector"
  ],
  "engines": {
    "node": ">=22.5.0"
  },
  "license": "MIT"
}
